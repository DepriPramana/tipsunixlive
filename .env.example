# Example environment variables
# Copy this file to .env and fill in your values

# YouTube Stream Key (required)
# Get this from YouTube Studio > Go Live > Stream Key
YOUTUBE_STREAM_KEY=your-youtube-stream-key-here

# FFmpeg Path (optional, default: ffmpeg)
# If FFmpeg is not in PATH, specify full path
FFMPEG_PATH=ffmpeg

# Database URL (optional, default: sqlite:///./data.db)
DATABASE_URL=sqlite:///./data.db
